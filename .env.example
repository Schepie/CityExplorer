# Foursquare API Key (for basic POI searching)
VITE_FOURSQUARE_KEY=your_foursquare_api_key_here

# Google API Key (used for Places API, Knowledge Graph, and custom search)
# Note: Ensure this key has 'Places API' and 'Custom Search API' enabled in Google Cloud Console.
VITE_GOOGLE_PLACES_KEY=your_google_places_api_key_here

# Google Programmable Search Engine ID (CX)
# Create one at: https://programmablesearchengine.google.com/
# This is NOT your API key, but a specific engine ID (e.g., 'a1b2c3d4e5f6g7h8i').
VITE_GOOGLE_SEARCH_CX=your_google_search_cx_id_here

# Gemini API Key (for AI descriptions)
# Get one at: https://aistudio.google.com/app/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key_here
